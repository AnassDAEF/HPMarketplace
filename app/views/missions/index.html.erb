<p id="notice"><%= notice %></p>

<h1>Missions</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Sub title</th>
      <th>Description</th>
      <th>Start date</th>
      <th>Finish date</th>
      <th>Deadline date</th>
      <th>Id object</th>
      <th>Id user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missions.each do |mission| %>
      <tr>
        <td><%= mission.title %></td>
        <td><%= mission.sub_title %></td>
        <td><%= mission.description %></td>
        <td><%= mission.start_date %></td>
        <td><%= mission.finish_date %></td>
        <td><%= mission.deadline_date %></td>
        <td><%= mission.id_object %></td>
        <td><%= mission.id_user %></td>
        <td><%= link_to 'Show', mission %></td>
        <td><%= link_to 'Edit', edit_mission_path(mission) %></td>
        <td><%= link_to 'Destroy', mission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mission', new_mission_path %>
